tag=zeekssh ax auth_success != "-" client origiantor auth_attempts responder host_key
| stats count sum(auth_attempts) as attempts by client responder auth_success
| eval if(auth_successi == "T"){success = count;}
| geoip originator.CountryName originator.City
| stats sum(success) as success by client responder
| eval success > 0
| eval if ((float(attempts)/float(success)) > 2.0) { Notes = "Potential Brute Force Success"; }
| sort by success desc
| table responder originator host_key CountryName City client attempts success Notes
