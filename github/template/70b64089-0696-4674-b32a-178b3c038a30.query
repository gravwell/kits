tag=$GITHUB_AUDIT json SRC user actor actor_ip repo action~"pull" operation_type
| eval if (!has(actor_ip) || actor_ip == "") {actor_ip = "No Data";}
| eval if (!has(user) || user == "") {user = "No Data";}
| eval actor_ip~"%%2%%"
| ip actor_ip
| geoip actor_ip.Location actor_ip.City actor_ip.Country actor_ip.CountryName
| stats unique_count by action
| numbercard unique_count