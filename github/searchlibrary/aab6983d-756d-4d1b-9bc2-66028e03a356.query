tag=$GITHUB_AUDIT json @timestamp actor action operation_type business repo
| eval in(action, 
    "business_secret_scanning_push_protection.disabled_for_new_repos"
    )
| printf -e context "at %v, %v was %v by %v in %v" @timestamp action operation_type actor business
| $GITHUB_ALERT_META("Medium", "Push Protection Bypass Disabled for New Repos")