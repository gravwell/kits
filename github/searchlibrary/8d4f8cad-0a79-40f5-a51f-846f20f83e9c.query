tag=$GITHUB_AUDIT json @timestamp actor action repo operation_type
| eval in(action, 
    "business_advanced_security.user_namespace_repos_disabled"
    )
| printf -e context "at %s, %s was %s by %s in %s" @timestamp action operation_type actor repo
| $GITHUB_ALERT_META("High", "GitHub Advanced Security Disabled for User Namespace Repos in Enterprise")