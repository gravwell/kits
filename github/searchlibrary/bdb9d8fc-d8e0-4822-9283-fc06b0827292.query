tag=$GITHUB json @timestamp actor action operation_type business repo
| eval in(action, 
    "org.secret_scanning_push_protection_new_repos_disable"
    )
| printf -e context "at %v, %v was %v by %v in %v" @timestamp action operation_type actor business
| $GITHUB_ALERT_META("Medium", "Push Protection for Secret Scanning Disabled for New Repos")