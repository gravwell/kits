{
	"UUID": "3ba6dde2-cbed-4649-b878-2178e9883134",
	"Name": "Template - Okta - Investigation - Modification Alerts for Actor, Target, and/or IP [table]",
	"Description": "This template returns all Modification Alerts found within Okta System Log events associated with a specific actor, target, and/or IP address.\n\nIt extracts and normalizes key Okta fields and filters the results  based on user-supplied wildcard parameters:\n  - %%okta_actor%% - matches actor alternate ID or display name\n  - %%okta_target%% - matches target alternate ID or display name\n  - %%okta_ip%% - matches client IP or request IP\n\nReference(s):\n  - Okta \n    - https://developer.okta.com/docs/reference/api/event-types/\n    - https://developer.okta.com/docs/api/openapi/okta-management/management/tag/SystemLog/",
	"Data": {
		"variables": [
			{
				"name": "%%1%%",
				"label": "Okta Actor",
				"description": "This is the user (actorAltId or actorName) who performed the action.",
				"required": true,
				"defaultValue": "*",
				"previewValue": "*"
			},
			{
				"name": "%%2%%",
				"label": "Okta Target",
				"description": "This is the user (targetAltId or targetName) whom the action was performed against.",
				"required": true,
				"defaultValue": "*",
				"previewValue": "*"
			},
			{
				"name": "%%3%%",
				"label": "Okta IP",
				"description": "This is either the clientIp or the requestIp from which the action was performed.",
				"required": true,
				"defaultValue": "*",
				"previewValue": "*"
			}
		]
	},
	"Labels": [
		"okta"
	]
}